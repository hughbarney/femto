# Note: n-aryity shall be implemented in Lisp.
IN='(d+)' OUT='0' flisp_expr; ok add-1 add nothing is 0
IN='(d+ 42.0)' OUT='42.0' flisp_expr; ok add-2 add 42  is 42
IN='(d+ 1 2 3)' OUT='6' flisp_expr; ok add-4 add 1 2 3 is 6
IN='(d-)' OUT='0' flisp_expr; ok minus-1 minus nothing is 0
IN='(d- 42)' OUT='-42' flisp_expr; ok minus-2 minus 42  is -42
IN='(d- 1 2 3)' OUT='-4' flisp_expr; ok minus-4 minus 1 2 3 is -4
IN='(d*)' OUT='1' flisp_expr; ok times-1 times nothing is 1
IN='(d* 42)' OUT='42' flisp_expr; ok times-2 times 42  is 42
IN='(d* 1 2 3)' OUT='6' flisp_expr; ok times-4 times 1 2 3 is 6
IN='(d/ 42)' OUT='0.0238095' flisp_expr; ok divide-1 divide 42  is 1/42
IN='(d/ 42 7 6)' OUT='1' flisp_expr; ok divide-3 divide 42 7 6 is 1
IN='(i% 42)' OUT='1' flisp_expr; ok mod-1 mod 42  is 1
IN='(d% 11 6 3)' OUT='2' flisp_expr; ok mod-3 mod 11 6 3 is 2
IN='(d= 42)' OUT='t' flisp_expr; ok arith-equal-1 arith-equal 42  is t
IN='(d= 7 7 7)' OUT='t' flisp_expr; ok arith-equal-3 arith-equal 7 7 7 is t
IN='(d= 7 7 8)' OUT='nil' flisp_expr; ok arith-equal-4 arith-equal 7 7 8 is nil
IN='(d< 42)' OUT='t' flisp_expr; ok less-1 less 42  is t
IN='(d< 7 7 7)' OUT='nil' flisp_expr; ok less-3 less 7 7 7 is nil
IN='(d< 1 2 3)' OUT='t' flisp_expr; ok less-4 less 1 2 3 is t
IN='(<= 42)' OUT='t' flisp_expr; ok less-equal-1 less-equal 42  is t
IN='(d<= 7 7 7)' OUT='t' flisp_expr; ok less-equal-3 less-equal 7 7 7 is t
IN='(d<= 7 7 6)' OUT='nil' flisp_expr; ok less-equal-4 less-equal 7 7 6 is nil
IN='(d> 42)' OUT='t' flisp_expr; ok greater-1 greater 42  is t
IN='(d> 7 7 7)' OUT='nil' flisp_expr; ok greater-3 greater 7 7 7 is nil
IN='(d> 7 7 6)' OUT='nil' flisp_expr; ok greater-4 greater 7 7 6 is nil
IN='(i>= 42)' OUT='t' flisp_expr; ok greater-equal-1 greater-equal 42  is t
IN='(d>= 7 7 7)' OUT='t' flisp_expr; ok greater-equal-3 greater-equal 7 7 7 is t
IN='(d>= 7.0 7.0 6.0)' OUT='t' flisp_expr; ok greater-equal-4 greater-equal 7 7 6 is t
